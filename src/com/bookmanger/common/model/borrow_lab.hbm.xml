<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bookmanger.common.model.Borrow_lab" table="borrow_lab">
		<id name="bookid" type="string">
			<column name="book_id" />
			<generator class="native" />
		</id>
		<property name="kucun" type="int" length="4" >
				<column name="kucun" >
                <comment>库存</comment>
                </column>
		</property>
		<property name="return_time" type="date" length="20" >
				<column name="return_time" >
                <comment>时间</comment>
                </column>
		</property>
	</class>
</hibernate-mapping>
